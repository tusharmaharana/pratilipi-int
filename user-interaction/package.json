{
  "name": "user-interaction-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.5.4",
    "@grpc/proto-loader": "^0.6.9",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/google-protobuf": "^3.15.5",
    "@types/lodash": "^4.14.178",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.15",
    "dotenv": "^16.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start": "ts-node -r dotenv/config src/index.ts",
    "start:grpc": "ts-node proto-server/index.ts",
    "proto:gen": "sh proto/build.sh",
    "dev:grpc": "nodemon -r dotenv/config proto-server/index.ts",
    "dev:service": "nodemon -r dotenv/config src/index.ts",
    "dev": "ts-node -r dotenv/config proto-server/index.ts & ts-node -r dotenv/config src/index.ts"
  }
